{
  "private": true,
  "name": "npm-seed",
  "version": "0.0.0",
  "description": "A project template for authoring and publishing npm package",
  "repository": "4f2e/npm-seed",
  "author": "xingyuzhe <yuzhe.xing@qq.com>",
  "contributors": [
    "xingyuzhe <yuzhe.xing@qq.com>"
  ],
  "license": "MIT",
  "keywords": [
    "npm",
    "package",
    "seed",
    "boilerplate"
  ],
  "main": "index.js",
  "jsnext:main": "index.es.js",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base",
    "rules": {
      "import/no-mutable-exports": 0
    }
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-plugin-external-helpers": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-preset-es2015": "*",
    "babel-preset-es2015-rollup": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "babelrc-rollup": "*",
    "chai": "*",
    "colors": "*",
    "coveralls": "*",
    "del": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "gzip-size": "*",
    "istanbul": "*",
    "mocha": "*",
    "rollup": "*",
    "rollup-plugin-babel": "*",
    "rollup-plugin-json": "*",
    "sinon": "*",
    "uglify-js": "*"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo kriasoft/babel-starter-kit",
    "start": "easystatic start docs"
  },
  "dependencies": {
    "babel-runtime": "*"
  }
}
