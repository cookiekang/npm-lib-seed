{
  "private": true,
  "name": "npm-seed",
  "version": "0.0.0",
  "description": "A project for report javascript error and page performance",
  "repository": "4f2e/npm-seed",
  "author": "xingyuzhe <yuzhe.xing@qq.com>",
  "contributors": [
    "xingyuzhe <yuzhe.xing@qq.com>"
  ],
  "license": "MIT",
  "keywords": [
    "javascript",
    "error",
    "report",
    "performance",
    "page"
  ],
  "main": "index.js",
  "jsnext:main": "index.es.js",
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb/base",
    "rules": {
      "import/no-mutable-exports": 0,
      "import/no-extraneous-dependencies": 0,
      "semi": 0,
      "arrow-parens": 0,
      "prefer-template": 0,
      "no-underscore-dangle": 0,
      "prefer-const": 0,
      "no-param-reassign": 0,
      "prefer-rest-params": 0,
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ]
    }
  },
  "devDependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-eslint": "*",
    "babel-plugin-external-helpers": "*",
    "babel-plugin-transform-runtime": "*",
    "babel-preset-es2015": "*",
    "babel-preset-es2015-rollup": "*",
    "babel-preset-stage-0": "*",
    "babel-register": "*",
    "babelrc-rollup": "*",
    "bs-html-injector": "*",
    "chai": "*",
    "colors": "*",
    "coveralls": "*",
    "del": "*",
    "eslint": "*",
    "eslint-config-airbnb": "*",
    "eslint-plugin-import": "*",
    "eslint-plugin-jsx-a11y": "*",
    "eslint-plugin-react": "*",
    "gzip-size": "*",
    "istanbul": "*",
    "mocha": "*",
    "rollup": "*",
    "rollup-plugin-babel": "*",
    "rollup-plugin-commonjs": "^5.0.0",
    "rollup-plugin-json": "*",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "*",
    "sinon": "*"
  },
  "scripts": {
    "lint": "eslint src test tools",
    "test": "mocha --compilers js:babel-register",
    "test:watch": "mocha --compilers js:babel-register --reporter min --watch",
    "test:cover": "babel-node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "babel-node tools/build",
    "prepublish": "npm run build",
    "publish:docs": "easystatic deploy docs --repo 4f2e/npm-seed",
    "start": "easystatic start docs"
  },
  "dependencies": {}
}
